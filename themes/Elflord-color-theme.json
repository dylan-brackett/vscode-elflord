{
	"name": "Elflord",
	"colors": {
		// Editor
		"editor.background": "#000000",
		"editor.foreground": "#00ffff",
		// Activity Bar
		"activityBarBadge.background": "#007acc",
		"activityBar.background": "#000000",
		// Status Bar 
		"statusBar.background": "#7f7f7f",
		"statusBar.foreground": "#ffffff",
		"statusBarItem.remoteBackground": "#7f7f7f",
		"statusBarItem.remoteForeground": "#ffffff",
		// Sidebar
		"sideBar.background": "#000000",
		"sideBar.foreground": "#ffffff",
		"sideBarSectionHeader.background": "#000000",
		"sideBarSectionHeader.foreground": "#ffffff",
		"sideBarTitle.foreground": "#00ffff",
		// Line Numbers
		"editorLineNumber.foreground": "#ffff00",
		"editorLineNumber.activeForeground": "#ffff00",
		// Cursor
		"editorCursor.background": "#00ffff",
		"editorCursor.foreground": "#000000",
		// Title Bar
		"titleBar.activeBackground": "#000000",
		"titleBar.activeForeground": "#ffffff",
		// Tabs
		"editorGroupHeader.tabsBackground": "#7f7f7f",
		"tab.inactiveBackground": "#7f7f7f",
		"tab.inactiveForeground": "#ffffff",
		// Buttons
		"button.background": "#ffffff",
		"button.foreground": "#000000",
	},
	"tokenColors": [
		{
			"name": "Constant",
			"scope": [
				"constant",
				"string"
			],
			"settings": {
				"foreground": "#FF0000"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
			],
			"settings": {
				"foreground": "#80a0ff"
			}
		},
		{
			"name": "Conditional",
			"scope": [
				"keyword.control",
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Function",
			"scope": [
				"entity.name.function",
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Identifier",
			"scope": [
				"entity.name.type",
				"entity.name.class",
				"entity.name.variable",
				"entity.name.tag",
				"entity.name.section",
				"entity.name.filename",
				"entity.name.package",
				"entity.name.type.class",
				"entity.name.type.enum",
				"entity.name.type.interface",
				"entity.name.type.module",
				"entity.name.type.namespace",
				"entity.name.type.struct",
				"entity.name.type.union",
				"entity.name.type.variable",
				"entity.name.type.alias",
				"entity.name.type.template",
				"entity.name.type.literal",
				"entity.name.type.enum-member",
				"entity.name.type.parameter",
				"entity.name.type.template-parameter",
				"entity.name.type.function",
				"entity.name.type.property",
				"entity.name.type.typedef",
				"entity.name.type.variable",
				"entity.name.type.enum",
				"entity.name.type.macro",
				"entity.name.type.primitive",
				"entity.name.type.annotation",
				"entity.name.type.mixin",
				"entity.name.type.template",
				"entity.name.type.import",
				"entity.name.type.module",
			],
			"settings": {
				"foreground": "#40ffff"
			}
		},
		{
			"name": "Type",
			"scope": [
				"meta.type",
			],
			"settings": {
				"foreground": "#60ff60"
			}
		},
		{
			"name": "Search",
			"scope": [
				"entity.name.function.search",
			],
			"settings": {
				"foreground": "#ffff00",
			}
		},
		{
			"name": "Todo",
			"scope": [
				"meta.todo",
			],
			"settings": {
				"foreground": "#ffff00",
			}
		}
	]
}
		
